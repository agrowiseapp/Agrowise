{
  "expo": {
    "name": "agrowise",
    "slug": "agrowise",
    "version": "1.1.4",
    "scheme": "agrowise",
    "orientation": "portrait",
    "icon": "./assets/Logo/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo/logo-color.png",
      "resizeMode": "contain",
      "backgroundColor": "#628479"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "aggro.application",
      "buildNumber": "24",
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./assets/Logo/icon.png",
      "infoPlist": {
        "CFBundleIconName": "AppIcon"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "aggro.application",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo/icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "versionCode": 49
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aba9a469-c1ec-4d74-a839-0f3696305c1a"
      }
    },
    "owner": "agrowiseapp",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "targetSdkVersion": 35,
            "newArchEnabled": true,
            "ndkVersion": "26.1.10909125"
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1",
            "newArchEnabled": true,
            "extraPodfileProperties": {
              "CLANG_WARN_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "NO",
              "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
              "CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER": "NO"
            }
          }
        }
      ],
      "expo-web-browser"
    ]
  }
}
