{
  "cli": {
    "version": ">= 5.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.19.4",
      "prebuildCommand": "npx expo prebuild --clean",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.19.4",
      "prebuildCommand": "npx expo prebuild --clean",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "node": "20.19.4",
      "prebuildCommand": "npx expo prebuild --clean",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_USE_APPLE_DEVICE_SUPPORT": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
